docker login container-registry.oracle.com

docker pull container-registry.oracle.com/database/enterprise:latest

mkdir /home/prashanto/Documents/docker/database
sudo chown oracle:prashanto /home/prashanto/Documents/docker/database
sudo chmod -R 775 /home/prashanto/Documents/docker/database

sudo chown -R oracle:prashanto /home/prashanto/Documents/docker/database

docker run -d --name oracledb \
 -p 1521:1521 -p 5500:5500 \
 -e ORACLE_SID=pkdb \
 -e ORACLE_PDB=pkpdb \
 -e ORACLE_PWD=Oracle@2707\
 -e INIT_SGA_SIZE=4096 \
 -e INIT_PGA_SIZE=4096\
 -e ORACLE_EDITION=standard \
 -e ORACLE_CHARACTERSET=AL32UTF8 \
 -e ENABLE_ARCHIVELOG=false \
 --shm-size=8g \
 --memory=8g \
container-registry.oracle.com/database/enterprise:latest


 --volume /home/prashanto/Documents/docker/database:/opt/oracle/oradata \



